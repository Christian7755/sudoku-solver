{
    "compileOnSave": false,
    "compilerOptions": {
      "outDir": "./out-tsc",
      "sourceMap": true,
      "declaration": false,
      "moduleResolution": "node",
      "target": "es2022",
      "module": "es2022",
      "lib": ["es2021", "dom"],
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowJs": true,
      "strict": true,
      "resolveJsonModule": true,
      "experimentalDecorators": true,
      "allowSyntheticDefaultImports": true
    }
  }
  